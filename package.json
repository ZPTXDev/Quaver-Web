		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vitest": "^0.30.1"
	},
	"type": "module",
	"dependencies": {
		"@zptxdev/zptx-lib": "^1.1.0",
		"patch-package": "^6.5.1",
		"socket.io-client": "^4.6.1",
		"svelte-toasts": "^1.1.2"
